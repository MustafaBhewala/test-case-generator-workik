{
  "version": 2,
  "functions": {
    "api/auth.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/github.js": {
      "runtime": "@vercel/node@18.x"
    },
    "api/ai.js": {
      "runtime": "@vercel/node@18.x"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth",
      "dest": "/api/auth.js"
    },
    {
      "src": "/api/github/(.*)",
      "dest": "/api/github.js"
    },
    {
      "src": "/api/ai/(.*)",
      "dest": "/api/ai.js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
